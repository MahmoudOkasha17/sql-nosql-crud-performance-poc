{
  "name": "sql-nosql-crud-performance-poc",
  "version": "1.0.0",
  "description": "Project to test the performance difference between postgres + sequelize vs mongodb + mongoose.",
  "main": "./src/app.ts",
  "scripts": {
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "status": "tsc --p tsconfig.json --listFiles",
    "test": "jest",
    "start": "node -r module-alias/register ./dist/app.js",
    "build": "tsc -p ./tsconfig.prod.json",
    "dev": "nodemon --watch 'src/**/*.ts' --exec ts-node -r tsconfig-paths/register src/app.ts"
  },
  "_moduleAliases": {
    "@/routes": "dist/routes",
    "@/types": "dist/types",
    "@/data": "dist/data",
    "@/middleware": "dist/middleware",
    "@/controllers": "dist/controllers",
    "@/db": "dist/db",
    "@/utils": "dist/utils",
    "@/config": "dist/config",
    "@/models": "dist/models",
    "@/schemas": "dist/schemas"
  },
  "author": "Mahmoud Okasha",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "9.0.0",
    "cors": "2.8.5",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "express-rate-limit": "7.4.0",
    "helmet": "7.1.0",
    "hpp": "0.2.3",
    "module-alias": "2.2.2",
    "mongoose": "8.6.2",
    "morgan": "1.10.0",
    "pg": "8.12.0",
    "pg-hstore": "2.3.4",
    "sequelize": "6.37.3",
    "xss": "1.0.14",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@types/hpp": "0.2.2",
    "@types/jest": "29.5.1",
    "@types/morgan": "1.9.4",
    "@types/node": "20.14.8",
    "jest": "29.5.0",
    "nodemon": "3.1.4",
    "prettier": "3.3.3",
    "sequelize-cli": "6.6.2",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.6.2"
  }
}
